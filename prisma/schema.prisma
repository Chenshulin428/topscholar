generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Paper {
  id         Int      @id @default(autoincrement())
  year       Int
  volume     String?
  issue      String?
  titleEn    String   @db.Text
  titleCn    String?  @db.Text
  authors    Json?
  abstractEn String?  @db.Text
  abstractCn String?  @db.Text
  journal    String
  doi        String   @unique
  url        String?
  createdAt  DateTime @default(now())
}
